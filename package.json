{
  "name": "wayback-machine-downloader",
  "version": "1.0.0",
  "description": "Download web pages from the Wayback Machine",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:trace": "node --trace-deprecation --import tsx src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --check",
    "format:write": "prettier . --write"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^14.0.0",
    "p-queue": "^8.1.0",
    "progress": "^2.0.3"
  },
  "devDependencies": {
    "@types/node": "^24.2.0",
    "@types/progress": "^2.0.7",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2"
  }
}
